{
	"compilerOptions": {
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react",
		"module": "es6",
		"moduleResolution": "node",
		"noImplicitAny": false,
		"allowSyntheticDefaultImports": true,
		"outDir": "./dist",
		"preserveConstEnums": true,
		"target": "es5"
	},
	"exclude": ["node_modules"],
	"include": ["src"]
}
